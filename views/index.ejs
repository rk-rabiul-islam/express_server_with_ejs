

    <div class="container my-5">
      <div class="row justify-content-center">
        <div class="col-md-8">
          <div class="card">
            <div class="card-header">
              <h1>All Student Data</h1>
            </div>
            <div class="card-body all_student_page">
              <table class="table table-striped table-bordered">
                <thead>
                  <tr>
                    <th>#</th>
                    <th>Name</th>
                    <th>Email</th>
                    <th>Phone</th>
                    <th>Photo</th>
                    <th>Action</th>
                  </tr>
                </thead>
                <tbody>
                  <% students.forEach(( element, index ) => { %>
                    <tr>
                      <td><%= index + 1 %> </td>
                      <td><%= element.name %></td>
                      <td><%= element.email %></td>
                      <td><%= element.phone %></td>
                      <td><img src="/assets/upload/<%= element.photo %> " alt=""></td>
                      <td>
                        <a class="btn-sm btn-primary" href="/student/<%= element._id %>"><i class="fa fa-eye" aria-hidden="true"></i></a>
                        <a class="btn-sm btn-info" href="/student/edit/<%= element._id %>"><i class="fas fa-edit aria-hidden="true" "></i></a>
                        <a class="btn-sm btn-danger" href="/student/delete/<%= element._id %>"><i class="fas fa-trash "></i></a>
                      </td>
                    </tr>                   
                  <% }) %>
                </tbody>
              </table>
            </div>
            <div class="card-footer">
              <a href="/student/create" class="btn btn-primary">Create New Student</a>
            </div>
          </div>
        </div>
      </div>
    </div>
    
